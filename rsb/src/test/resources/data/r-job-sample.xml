                        <NCEs>654487</NCEs>         <!-- bone sample: NCEs -->
                        <MN-NCEs>578</MN-NCEs>      <!-- bone sample: /MN-NCEs -->
                    </animal>
                    <animal nr="103">
                    <!-- ... -->
                    </animal>
                    <!-- ... -->
                </dosageGroup>

                <dosageGroup code="L">
                <!-- ... -->
                </dosageGroup>
                <!-- ... -->
            </sampleResults>
        </MAAS>

    </statBody>

</statDataPackage>
